/*line-color*/
.b-rate {
  display: flex;
  line-height: 0;
  font-size: 0;
  flex-direction: row;
}
.b-rate__icon {
  position: relative;
  line-height: 0;
  font-size: 0;
}
.b-rate__icon-on {
  overflow: hidden;
  position: absolute;
  top: 0;
  left: 0;
  line-height: 1;
  text-align: left;
}
.b-rate--offstar {
  background-image: url(data:image/png;base64,);
  background-size: cover;
  background-repeat: no-repeat;
}
.b-rate--onstar {
  background-image: url(data:image/png;base64,);
  background-size: cover;
  background-repeat: no-repeat;
}

