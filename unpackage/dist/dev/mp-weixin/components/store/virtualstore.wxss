/*line-color*/
.store {
  background-color: #ffffff;
}
.store .imgs {
  padding: 40rpx 0 40rpx 32rpx;
}
.store .imgs image {
  width: 600rpx;
  height: 350rpx;
  margin-right: 32rpx;
}
.store .tag-order {
  font-size: 28rpx;
  color: #999999;
}
.store .store-bar {
  padding: 32rpx 32rpx 40rpx 32rpx;
  margin-bottom: 24rpx;
}
.store .store-bar .name {
  font-size: 34rpx;
  color: #020202;
  font-weight: 500;
  line-height: 48rpx;
}
.store .store-bar .rate {
  margin-top: 10rpx;
}
.store .store-bar .tag {
  margin: 19rpx 0 40rpx 0;
}
.store .store-bar .msg {
  font-size: 24rpx;
  color: #666666;
  font-weight: 300;
  line-height: 33rpx;
}
.store .store-bar .msg .time {
  font-size: 26rpx;
  color: #999999;
  font-weight: 400;
  line-height: 37rpx;
  margin-bottom: 16rpx;
}
.store .store-bar .msg .time text {
  font-size: 28rpx;
  line-height: 34rpx;
}
.store .store-bar .msg .safeguard {
  margin-top: 24rpx;
}
.store .store-bar .msg .phone {
  width: 64rpx;
  height: 64rpx;
  margin-left: 92rpx;
  background: url(data:image/png;base64,) center no-repeat;
  background-size: cover;
}
.goods .nav .text {
  position: relative;
  line-height: 54rpx;
  font-size: 32rpx;
}
.goods .nav .nav-on .text {
  font-weight: 500;
  color: #1D1D1D;
}
.goods .nav .nav-on .text::before {
  content: '';
  position: absolute;
  bottom: -14rpx;
  left: 50%;
  width: 40rpx;
  height: 6rpx;
  background-color: #1D1D1D;
  -webkit-transform: translateX(-50%);
          transform: translateX(-50%);
  border-radius: 6rpx;
}
.goods .goods-list {
  padding: 36rpx 12rpx 0rpx 12rpx;
}
.goods .goods-list .store-item {
  display: block;
  margin-bottom: 24rpx;
}
.goods .goods-list .store-item .store-item__detail {
  display: block;
  margin: 0 12rpx;
}
.panel {
  margin-bottom: 24rpx;
  padding: 0 24rpx 24rpx;
}
.panel .header {
  line-height: 45rpx;
  padding: 24rpx 0;
}
.panel .header text {
  font-size: 34rpx;
  color: #1D1D1D;
  font-weight: 500;
  vertical-align: middle;
}
.panel .comment-header {
  background: #ffffff;
  border-top: 1rpx solid #e5e5e5;
  border-bottom: 1rpx solid #e5e5e5;
  width: 686rpx;
  height: 120rpx;
  font-size: 30rpx;
  color: #666666;
  font-weight: 500;
}
.panel .comment-header .comment-good {
  font-weight: 400;
}
.panel .comment-header .comment-good text {
  font-size: 32rpx;
  color: #ff681d;
  margin: 0 12rpx;
}
.panel .comment-header .comment-good .cuIcon-right {
  display: inline-block;
}
.moreComment {
  margin-top: -20rpx;
  padding: 1rpx 0 32rpx 0;
}
.comment {
  padding: 0 0 36rpx;
  background-color: #FFF;
}
.comment-collect {
  padding: 36rpx 0 12rpx;
  margin: 0 24rpx;
  border-top: 1px solid #F2F2F2;
}
.comment-collect__item {
  display: inline-block;
  margin-right: 28rpx;
  background-color: #F7F7F7;
  border-radius: 28rpx;
  line-height: 52rpx;
  padding: 0 24rpx;
  color: #1D1D1D;
}
.comment-box {
  padding: 24rpx;
  border-bottom: 24rpx solid #F7F7F7;
}
.comment-box:last-child {
  border-bottom-width: 0;
}
.comment-box__nickname {
  margin-left: 12rpx;
  font-size: 26rpx;
}
.comment-box__content {
  margin-top: 36rpx;
  color: #666666;
  font-size: 26rpx;
}
.comment-box__img {
  margin-top: 24rpx;
  white-space: nowrap;
}
.comment-box__img-item {
  display: inline-block;
  width: 218rpx;
  height: 218rpx;
}
.comment-box__time {
  margin-top: 24rpx;
  color: #999999;
  font-size: 22rpx;
}
.panel-store {
  padding: 10rpx 0 32rpx 0;
}
.panel-store-item {
  display: block;
  margin-bottom: 24rpx;
}
.section {
  border-bottom: 24rpx solid #F7F7F7;
}
.section_title {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 36rpx 24rpx;
}
.section_title .title {
  font-size: 500;
  color: #1D1D1D;
  font-size: 32rpx;
}
.section_title .subtitle {
  color: #1D1D1D;
  font-size: 28rpx;
  font-weight: 400;
}

