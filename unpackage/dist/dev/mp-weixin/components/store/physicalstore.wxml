<view class="phycialstore"><view class="phycialstore-header"><view class="phycialstore-header__main flex align-center"><view class="cu-avatar round" style="{{'background-image:'+(storeDetail.cover_pic?'url('+storeDetail.cover_pic+')':'')+';'}}"></view><view class="info flex-sub"><view class="name">{{''+storeDetail.name}}</view><block wx:if="{{storeDetail.stars}}"><b-rate vue-id="17a5d655-1" value="{{storeDetail.stars}}" size="16" bind:__l="__l"></b-rate></block></view></view></view><view class="phycialstore-suggest"><view class="main-title phycialstore-suggest__title"><text class="title">为您推荐</text></view><view class="phycialstore-goods grid col-2 justify-between"><block wx:if="{{storeGoods.items.length}}"><block wx:for="{{storeGoods.items}}" wx:for-item="item" wx:for-index="__i0__" wx:key="id"><view class="store-items"><card class="item" vue-id="{{'17a5d655-2-'+__i0__}}" url="{{'/pages/goods/goods?id='+item.id+'&type='+item.product_type}}" img="{{item.cover_pic}}" name="{{item.name}}" price="{{item.price}}" originPrice="{{item.market_price}}" description="{{''+(item.total_sale>0?item.total_sale+'人订购':'')}}" bind:__l="__l"></card></view></block></block><block wx:else><text class="store-empty text-center">暂无推荐</text></block></view></view></view>