.rate .vw {
  display: inline-block;
}
.rate .text {
  font-size: 24rpx;
  color: #666666;
  margin-left: 10rpx;
  font-weight: 400;
  height: 32rpx;
}
.rate .text text {
  font-size: 28rpx;
  margin-right: 4rpx;
}
.rate .star {
  width: 32rpx;
  height: 32rpx;
  display: inline-block;
  margin-right: 10rpx;
}
.rate .star:last-child {
  margin-right: 0;
}
