/*line-color*/
.b-cardstore {
  padding: 28rpx;
  background-color: #FFF;
  border-radius: 24rpx;
}
.b-cardstore__info {
  position: relative;
  padding-left: 104rpx;
  padding-right: 132rpx;
  height: 88rpx;
}
.b-cardstore__avatar {
  width: 88rpx;
  height: 88rpx;
  position: absolute;
  top: 0;
  left: 0;
  background: url(data:image/png;base64,) center no-repeat;
  background-size: cover;
}
.b-cardstore__avatar-main {
  width: 100%;
  height: 100%;
  border-radius: 100%;
}
.b-cardstore__name {
  display: block;
  color: #1D1D1D;
  font-size: 32rpx;
  margin-bottom: 8rpx;
  line-height: 45rpx;
  height: 45rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.b-cardstore__btn {
  position: absolute;
  top: 0;
  right: 0;
  width: 120rpx;
  height: 56rpx;
  line-height: 56rpx;
  text-align: center;
  border-radius: 32rpx;
}
.b-cardstore__imgs {
  margin-top: 56rpx;
  width: 100%;
  white-space: nowrap;
}
.b-cardstore__imgbox {
  display: inline-block;
  position: relative;
  margin-right: 24rpx;
  width: 200rpx;
  height: 200rpx;
  background-color: #F1F1F1;
}
.b-cardstore__imgbox:last-child {
  margin-right: 0;
}
.b-cardstore__imgbox-img {
  display: block;
  width: 100%;
  height: 100%;
}
.b-cardstore__imgbox-text {
  position: absolute;
  bottom: 0;
  left: 10rpx;
  right: 10rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  font-size: 20rpx;
  line-height: 28rpx;
  text-align: center;
}
.b-cardstore__imgbox-price {
  font-size: 34rpx;
  line-height: 48rpx;
}

