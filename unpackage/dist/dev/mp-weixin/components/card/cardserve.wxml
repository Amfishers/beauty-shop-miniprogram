<navigator hover-class="none" url="{{url}}"><view class="{{['b-cardserve',customClass]}}"><view class="b-cardserve__box"><view class="b-cardserve__img"><image class="b-cardserve__img-main" src="{{img}}" mode></image></view><view class="b-cardserve__info"><view data-event-opts="{{[['tap',[['phoneTap',['$event']]]]]}}" class="b-cardserve__phone" bindtap="__e"></view><view class="b-cardserve__title"><block wx:if="{{badge}}"><text class="b-cardserve__badge primary-btn">{{badge}}</text></block>{{''+name+''}}</view><block wx:if="{{rate}}"><b-rate class="b-cardserve__rate" vue-id="10cd1cab-1" value="{{rate}}" size="16" bind:__l="__l"></b-rate></block><view class="b-cardserve__desc">{{''+description+''}}</view><view class="b-cardserve__distance"><b-icon class="b-cardserve__distance--icon" vue-id="10cd1cab-2" icon="location-outline" fontSize="14" color="#D8D8D8" bind:__l="__l"></b-icon>{{''+distance+''}}</view></view></view><block wx:if="{{location}}"><view class="b-cardserve__location">地址：<text class="b-cardserve__location-main">{{location}}</text></view></block></view></navigator>