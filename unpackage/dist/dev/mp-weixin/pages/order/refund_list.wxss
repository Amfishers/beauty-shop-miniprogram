/*line-color*/
page {
  background: #F7F7F7;
}
.container {
  width: 686rpx;
  min-height: 148rpx;
  background-color: #ffffff;
  border-radius: 16rpx;
  font-size: 28rpx;
  color: #333333;
  font-weight: 400;
  padding: 30rpx 32rpx;
  margin: 28rpx auto;
}
.header {
  width: 750rpx;
  height: 280rpx;
  background: #1D1D1D;
  border-radius: 0 0 50% 50% / 0 0 40rpx 40rpx;
  margin-bottom: -100rpx;
}
.header .header-info {
  padding: 40rpx 20rpx 0 40rpx;
}
.header .status {
  font-size: 36rpx;
  color: #ffffff;
  font-weight: 500;
  line-height: 50rpx;
  margin-bottom: 12rpx;
}
.header .notice {
  font-size: 26rpx;
  color: #ffffff;
  font-weight: 400;
  line-height: 37rpx;
}
.header .status-icon {
  width: 312rpx;
  height: 240rpx;
}
.mb28 {
  margin-bottom: 28rpx;
}
.mb28:last-child {
  margin-bottom: 0;
}
.sub-msg {
  font-size: 30rpx;
  color: #1D1D1D;
}
.sub-msg > text {
  font-size: 24rpx;
  color: #1D1D1D;
}
.sub-msg > text text {
  font-size: 44rpx;
}
.record-header {
  padding-bottom: 30rpx;
  font-weight: 500;
  line-height: 42rpx;
  font-size: 30rpx;
  border-bottom: 1rpx solid #E5E5E5;
}
.mc-timeline {
  width: 100%;
  font-size: 28rpx;
  color: #666666;
}
.mc-timeline .mct-box .mct-box-header .mct-icon {
  width: 40rpx;
  height: 40rpx;
  margin-right: 40rpx;
  background-size: cover;
}
.mc-timeline .mct-box .mct-box-header image {
  width: 40rpx;
  height: 40rpx;
  margin-right: 40rpx;
}
.mc-timeline .mct-box .mct-box-content {
  margin-left: 18rpx;
  border-left: 4rpx solid #CA935E;
  padding-left: 59rpx;
  padding-bottom: 44rpx;
  margin-top: 8rpx;
  margin-bottom: 8rpx;
  color: #ccc;
}
.mc-timeline .mct-box .mct-box-content view {
  margin-bottom: 12rpx;
}
.mc-timeline .mct-box .mct-box-content view:last-child {
  margin-bottom: 0;
}
.mc-timeline .mct-box.cur .mct-box-header {
  color: #f14744;
}
.mc-timeline .mct-box.cur .mct-box-content {
  border-color: #f2f2f2;
}
.mc-timeline .mct-box.pending .mct-box-header {
  color: #cccccc;
}
.mc-timeline .mct-box.pending .mct-box-header .mct-icon {
  color: #cccccc;
}
.mc-timeline .mct-box.pending .mct-box-content {
  border-color: #f2f2f2;
  color: #cccccc;
}
.mc-timeline .mct-box:last-child .mct-box-content {
  border: none;
}
.mc-timeline .mct-box--active .title {
  color: #CA935E;
  font-size: 32rpx;
}
.mc-timeline .mct-box--finish .mct-icon {
  background-image: url(data:image/png;base64,);
}
.mc-timeline .mct-box--closed .mct-icon {
  background-image: url(data:image/png;base64,);
}
.mc-timeline .mct-box--pending .mct-icon {
  background-image: url(data:image/png;base64,);
}
.mc-timeline .mct-box--pending .mct-box-content {
  border-left-color: #F2F2F2;
}
.mc-timeline .mct-box--fail .mct-icon {
  background-image: url(data:image/png;base64,);
}
.mc-timeline .mct-box--unfinish {
  color: #ccc;
}
.mc-timeline .mct-box--unfinish .title {
  color: #CCCCCC;
}
.mc-timeline .mct-box--unfinish .mct-box-content {
  border-left-color: #ccc;
}
.mc-timeline .mct-box--unfinish .mct-icon {
  background-image: url(data:image/png;base64,);
}

