<view class="search"><cu-custom vue-id="3584c8c8-1" isBack="{{true}}" bgColor="bg-white-heavy" bind:__l="__l" vue-slots="{{['content']}}"><view slot="content">搜索</view></cu-custom><view class="search-bar"><view class="search-input"><b-icon vue-id="3584c8c8-2" icon="search" bind:__l="__l"></b-icon><input class="search-input__input" type="text" placeholder-class="input-placeholder" placeholder="{{placeholder}}" focus="{{inputfocus}}" confirm-type="search" data-event-opts="{{[['confirm',[['searchConfirm',['$event']]]],['input',[['__set_model',['','search','$event',[]]]]]]}}" value="{{search}}" bindconfirm="__e" bindinput="__e"/></view><view data-event-opts="{{[['tap',[['cancelSearch',['$event']]]]]}}" class="search-cancel" bindtap="__e">取消</view></view><view class="main-pad"><view class="search-recommend search-history"><view class="search-title"><text class="title">历史搜索</text><b-icon vue-id="3584c8c8-3" icon="delete-outline" font-size="18" data-event-opts="{{[['^tap',[['resetHistory']]]]}}" bind:tap="__e" bind:__l="__l"></b-icon></view><view class="search-item"><block wx:if="{{history.length}}"><block wx:for="{{history}}" wx:for-item="his" wx:for-index="index" wx:key="*this"><view data-event-opts="{{[['tap',[['linkToSearch',['$0'],[[['history','',index]]]]]]]}}" class="item" bindtap="__e">{{''+his+''}}</view></block></block><block wx:else><view class="search-empty">暂无搜索记录~</view></block></view></view><view class="search-recommend"><view class="search-title"><text class="title">推荐搜索</text></view><view class="search-item"><block wx:if="{{recommend.length}}"><block wx:for="{{recommend}}" wx:for-item="rec" wx:for-index="index" wx:key="*this"><view data-event-opts="{{[['tap',[['linkToSearch',['$0'],[[['recommend','',index]]]]]]]}}" class="item" bindtap="__e">{{''+rec+''}}</view></block></block><block wx:else><view class="search-empty">暂无推荐内容~</view></block></view></view></view></view>