/*line-color*/
page {
  overflow: hidden;
}
.text-main {
  color: #f14744;
}
.cate-item {
  padding: 0 0 36rpx;
  text-align: center;
}
.pic {
  width: 90rpx;
  height: 90rpx;
}
.catename {
  line-height: 34rpx;
  margin-top: 12rpx;
  color: #666;
  font-size: 24rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.cinfo {
  margin: 14rpx 0;
}
.g3 {
  width: 33.333333%;
  text-align: center;
  height: 33.333333%;
}
.fixed {
  position: fixed;
  z-index: 99;
}
.VerticalNav.nav {
  width: 200rpx;
  white-space: initial;
}
.VerticalNav.nav .cu-item {
  width: 100%;
  text-align: center;
  background-color: #fff;
  margin: 0;
  border: none;
  height: 50px;
  position: relative;
}
.VerticalNav.nav .cu-item.cur .cu-item-text {
  margin: 6px auto;
  background-color: #1D1D1D;
  border-radius: 28px;
  color: #F5DBC3;
  line-height: 38px;
}
.VerticalBox {
  position: absolute;
  left: 0;
  right: 0;
  bottom: calc(100rpx + env(safe-area-inset-bottom) / 2);
  display: flex;
  height: 100%;
}
.VerticalMain {
  background-color: #F7F7F7;
  flex: 1;
}
.VerticalMain-item {
  margin: 24rpx;
  background-color: #FFF;
  border-radius: 16rpx;
  overflow: hidden;
}
.cu-bar .action {
  color: #1D1D1D;
  font-size: 32rpx;
  font-weight: 500;
}

