/*line-color*/
.cashout-title {
  padding: 30rpx 0;
  font-size: 36rpx;
  line-height: 50rpx;
  color: #1D1D1D;
}
.cashout-bankcard {
  margin: 0 32rpx;
}
.cashout-bankcard__item {
  padding: 12rpx 0 42rpx;
}
.cashout-bankcard__item-info {
  font-size: 32rpx;
  color: #333;
}
.cashout-bankcard__item-img {
  margin-right: 16rpx;
  width: 58rpx;
  height: 58rpx;
}
.cashout-bankcard__item-num {
  display: block;
  color: #333;
  margin-top: 12rpx;
  font-size: 26rpx;
}
.cashout-bankcard__item-card {
  padding: 0 16rpx;
  flex: 1;
}
.cashout-bankcard__item .third-color {
  display: block;
  color: #999;
}
.cashout-bankcard__item .text {
  font-size: 32rpx;
  margin-right: 16rpx;
}
.cashout-bankcard__item .text-grey {
  color: #999;
  font-size: 28rpx;
}
.cashout-main {
  border-top: 28rpx solid #F2F2F2;
  padding: 0 32rpx;
}
.cashout-box__input {
  padding: 22rpx 0 10rpx;
  color: #1D1D1D;
}
.cashout-box__input .text {
  font-size: 64rpx;
  line-height: 90rpx;
}
.cashout-box__input .placeholder {
  line-height: 40rpx;
  font-weight: 400;
}
.cashout-box__input .input {
  margin-left: 24rpx;
  flex: 1;
  font-size: 64rpx;
  height: 90rpx;
  font-weight: 600;
  line-height: 90rpx;
}
.cashout-box__tip {
  border-top: 1rpx solid #e5e5e5;
  padding-top: 28rpx;
}
.cashout-box__tip .all {
  margin-left: 50rpx;
}
.cashout-btn {
  width: 100%;
  height: 96rpx;
  font-size: 32rpx;
  font-weight: 400;
  background-color: #1D1D1D !important;
  color: #F5DBC3 !important;
}
.cashout-record {
  margin-top: 32rpx;
  text-align: center;
}
.cashout-record .link {
  font-size: 26rpx;
  color: #999999;
  line-height: 37rpx;
  text-decoration: underline;
}
.cashout .placeholder {
  color: #999;
  font-size: 32rpx;
}
.phonecheck {
  margin: 72rpx 0 56rpx;
  padding: 60rpx 40rpx;
  border: 1rpx solid rgba(30, 30, 30, 0.1);
  border-radius: 24rpx;
}
.phonecheck__box {
  margin-top: 40rpx;
  padding: 0 36rpx;
  border: 1rpx solid rgba(30, 30, 30, 0.1);
  border-radius: 24rpx;
}
.phonecheck__box .detail {
  display: flex;
  width: 100%;
  padding: 60rpx 0;
}
.phonecheck__box .placeholder {
  color: #999;
  font-size: 28rpx;
  line-height: 40rpx;
}
.phonecheck__box-icon {
  width: 40rpx;
  height: 40rpx;
}
.phonecheck__box-captcha {
  position: relative;
}
.phonecheck__box-captcha::before {
  content: '';
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  width: 100%;
  height: 1px;
  background: rgba(30, 30, 30, 0.1);
}
.phonecheck__box-sentcode {
  color: #1D1D1D;
  font-size: 26rpx;
}
.phonecheck .input {
  flex: 1;
  margin-left: 40rpx;
}

