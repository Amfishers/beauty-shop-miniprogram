<view><cu-custom vue-id="719389aa-1" isBack="{{true}}" bgColor="bg-white-heavy" bind:__l="__l" vue-slots="{{['content']}}"><view slot="content">提现</view></cu-custom><view class="cashout"><view class="cashout-bankcard"><view class="cashout-title">收款人银行卡</view><view class="cashout-bankcard__item flex align-center justify-between"><image class="cashout-bankcard__item-img" src="../../static/cash-out.png" mode></image><block wx:if="{{selectedCard}}"><navigator class="flex-sub flex align-center" url="/pages/my/bankcard_list?type=select" hover-class="none"><view class="flex-sub flex align-center"><view class="cashout-bankcard__item-card"><view class="flex align-center"><text class="text">{{selectedCard.account_name||'无'}}</text><text class="text-grey">{{selectedCard.bank_name||'无'}}</text></view><view class="cashout-bankcard__item-num">{{''+selectedCard.bank_card+''}}</view></view><b-icon class="third-color" vue-id="719389aa-2" icon="right" font-size="14" bind:__l="__l"></b-icon></view></navigator></block><block wx:else><navigator class="flex-sub flex align-center" url="/pages/my/bankcard" hover-class="none"><view class="cashout-bankcard__item-card"><view class="flex align-center"><text class="text">绑定银行卡</text></view><view class="cashout-bankcard__item-num"><text class="text-grey">提现手续费率0.06%</text></view></view><b-icon class="third-color" vue-id="719389aa-3" icon="right" font-size="14" bind:__l="__l"></b-icon></navigator></block></view></view><view class="cashout-main"><view class="cashout-title">提现金额</view><view class="cashout-box"><view class="cashout-box__input flex"><text class="text">¥</text><input class="input" focus="isFocus" type="number" placeholder="提现手续费率0.06%" placeholder-class="placeholder" data-event-opts="{{[['input',[['__set_model',['','extract','$event',[]]]]]]}}" value="{{extract}}" bindinput="__e"/></view><view class="cashout-box__tip"><text>{{"余额 ¥"+(balance||0)}}</text><text data-event-opts="{{[['tap',[['allPickHandler',['$event']]]]]}}" class="all third-color" bindtap="__e">全部提现</text></view></view><view class="phonecheck flex align-center justify-between"><view class="phonecheck__box-icon"><b-icon vue-id="719389aa-4" icon="passwork" font-size="20" bind:__l="__l"></b-icon></view><input class="input flex-sub" placeholder-class="placeholder" focus="{{captchaInputFocus}}" type="number" placeholder="请输入验证码" data-event-opts="{{[['input',[['__set_model',['','userCaptcha','$event',[]]]]]]}}" value="{{userCaptcha}}" bindinput="__e"/><block wx:if="{{!captchaSent}}"><view data-event-opts="{{[['tap',[['sendCaptchaHanlder',['$event']]]]]}}" class="phonecheck__box-sentcode" bindtap="__e">获取验证码</view></block><block wx:else><view class="phonecheck__box-sentcode">{{''+countDownText+''}}</view></block></view><button class="cu-btn common-btn round cashout-btn" disabled="{{disabled}}" data-event-opts="{{[['tap',[['cashoutHandler',['$event']]]]]}}" bindtap="__e">提现</button><view class="cashout-record"><navigator class="link" url="/pages/my/cash_out_list">查看提现记录</navigator></view></view></view></view>