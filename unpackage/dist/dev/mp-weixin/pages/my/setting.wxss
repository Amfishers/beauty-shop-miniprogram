/*line-color*/
.index {
  background-color: #ffffff;
}
.list {
  padding-left: 32rpx;
}
.cu-list.menu > .cu-item {
  padding-left: 0;
  padding-right: 54rpx;
}
.cu-list.menu > .cu-item:last-child::after {
  border-bottom: 1rpx solid #dddddd;
}
.logout {
  width: 686rpx;
  height: 96rpx;
  background: #1D1D1D !important;
  color: #F5DBC3 !important;
  border-radius: 48rpx;
  margin: 64rpx 32rpx;
  font-size: 36rpx;
  color: #ffffff;
}
.nickname-modal {
  font-size: 36rpx;
  color: #000;
  font-weight: 400;
}
.nickname-modal .nickname-header {
  height: 160rpx;
}
.nickname-modal .nickname-header .nickname-header-subtitle {
  font-size: 24rpx;
  color: #999999;
  margin-top: 24rpx;
}
.nickname-modal .nickname-input {
  background-color: #f5f5f5;
  height: 72rpx;
  font-size: 28rpx;
  color: #333333;
}
.nickname-modal .modal-btn {
  font-size: 36rpx;
}
.uni-picker-container .uni-picker-action.uni-picker-action-confirm {
  color: #f14744;
}

