/*line-color*/
.my-card {
  margin: 24rpx 0;
  border-radius: 24rpx;
  background-color: #FFF;
}
.my-card__hd {
  height: 88rpx;
  line-height: 40rpx;
  padding: 24rpx;
  border-bottom: 1px solid #F2F2F2;
}
.my-card__hd .text {
  font-weight: bold;
  color: #1D1D1D;
}
.my-card__hd .link {
  color: #999;
  line-height: 34rpx;
  font-size: 24rpx;
}
.my-card__bd {
  padding: 8rpx 10rpx 40rpx;
}
.my-card__item {
  margin-top: 32rpx;
  text-align: center;
}
.my-card__item .iconfont {
  color: #BE865D;
}
.my-card__item .text {
  color: #666666;
  margin-top: 18rpx;
  font-size: 28rpx;
  line-height: 40rpx;
}
.my-card__item .num {
  font-size: 40rpx;
  line-height: 56rpx;
}
.my-card__service .iconfont {
  color: #1D1D1D;
}
page {
  background-color: #F7F7F7;
}
.my {
  padding-bottom: calc(100rpx + env(safe-area-inset-bottom) / 2);
}
.my-header {
  width: 100%;
  height: 478rpx;
  position: absolute;
  top: 0;
  left: 0;
  z-index: 1;
  background: #FFF;
  overflow: hidden;
  background-image: url(data:image/png;base64,);
  background-size: 100% 478rpx;
  background-repeat: no-repeat;
  background-position: center center;
}
.my-header .my-info {
  width: 100%;
  height: 200rpx;
  padding: 32rpx 45rpx;
}
.my-header .user {
  position: relative;
  z-index: 2;
  top: -40rpx;
  width: 100%;
}
.my-header .user .avatar {
  width: 128rpx;
  height: 128rpx;
  border-radius: 64rpx;
  margin-bottom: 22rpx;
  overflow: hidden;
}
.my-header .user .nickname {
  font-size: 36rpx;
  color: #ffffff;
}
.my-header .notice-bar {
  position: absolute;
  left: 0;
  z-index: 10000;
  width: 100%;
  height: 46rpx;
  padding-left: 32rpx;
}
.my-header .notice-bar image {
  margin-right: 59rpx;
  width: 46rpx;
  height: 46rpx;
}
.my-main {
  margin: 0 24rpx;
}
.user_info {
  width: 100%;
  height: 200rpx;
  padding: 32rpx 45rpx;
}
.user_avatar {
  width: 96rpx;
  height: 96rpx;
  margin-right: 27rpx;
}
.user_name {
  font-size: 36rpx;
  color: #ffffff;
}
.user_opt {
  width: 48rpx;
  height: 48rpx;
}
.plus_not_open {
  width: 686rpx;
  height: 128rpx;
  background: #FFFFF;
  margin: 0 auto;
  padding: 24rpx 32rpx;
  position: relative;
  background: linear-gradient(90deg, #f5dbc3 0%, #ca935e 100%, #ca935e 100%);
  border-radius: 32rpx 32rpx 0 0;
}
.plus_left {
  z-index: 2;
}
.plus_right {
  width: 147rpx;
  height: 56rpx;
  border-radius: 30rpx;
  z-index: 2;
}
.openNow {
  font-size: 26rpx;
  line-height: 56rpx;
  border-radius: 30rpx;
}
.bg-u {
  position: absolute;
  bottom: 0;
  height: 20rpx;
  width: 100%;
  background-image: url(data:image/png;base64,);
  z-index: 3;
  background-size: 100% 20rpx;
  background-repeat: no-repeat;
  background-position: center center;
}

